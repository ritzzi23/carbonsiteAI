# CarbonSiteAI Environment Variables
# Copy this file to .env and fill in your actual API keys

# Required API Keys
GROQ_API_KEY="your_groq_api_key_here"

# Optional API Keys (for enhanced features)
OPENAI_API_KEY="your_openai_api_key_here"

# Mapbox API Key (for interactive maps)
MAPBOX_ACCESS_TOKEN="your_mapbox_access_token_here"

# EPA API Configuration (free, no key required)
# EPA_ECHO_API_KEY=""  # Usually not required for public data

# Census API Configuration (free, no key required)
# CENSUS_API_KEY=""    # Usually not required for public data

# Application Configuration
DEBUG=True
LOG_LEVEL=INFO

# Database Configuration (if using local database)
# DATABASE_URL="sqlite:///./carbon_site_ai.db"

# Server Configuration
HOST="0.0.0.0"
PORT=8501 